		<script type="text/javascript">
      function check_reg_number()
      {
      	
        var rn = document.getElementById("AdvAppNo").value;
        if( rn.length < 12 || rn.length > 12 ) 
        {
          document.getElementById("AdvAppNo").value = "";
		  window.alert("Enter a valid registration number");
        }
        else
        {
          
            //var regNum_regExp = new RegExp("[A-Z]\d\{8\}");
           // Should contain exactly 8 digits
           
          var rExp = new RegExp("G[0-9]\{11\}");
          if( rExp.test(rn) === false )
          {
            document.getElementById("AdvAppNo").value = "";
			window.alert("Enter a valid registration number");
          }
        }

        
      }

      function check_dob(dob)
      {
        var c_dob = document.getElementById("datepicker").value;
		console.log(c_dob);
				var dob_array = c_dob.split("-");
        if( c_dob.length < 10 || c_dob.length > 10 ) 
        {
          document.getElementById("datepicker").value = "";
		  window.alert("Enter Date of Birth in the valid format");
        }
        else
        {
          var rExp = new RegExp("[0-9]\{2\}[-/][0-9]\{2\}[-/][0-9]\{4\}");
          if( rExp.test(c_dob) === false )
          {
            document.getElementById("datepicker").value = "";
			window.alert("Enter Date of Birth in the valid format");
          }
        }
      }

      function check_mobile()
      {
        var mobileNo = document.getElementById("mobileNo").value;
		if(mobileNo.length < 6 || mobileNo.length > 14){
			document.getElementById("mobileNo").value = "";
			window.alert("Enter a valid mobile number without country code (Only Numbers are allowed)");
		}
		var rExp = new RegExp("[0-9]");
		if( rExp.test(mobileNo) === false )
		{
			document.getElementById("mobileNo").value = "";
			window.alert("Enter a valid mobile number without country code (Only Numbers are allowed)");
		}
      }

